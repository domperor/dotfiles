[core]
	sparsecheckout = true
	pager =
[user]
	name = Koki Takahashi
	email = hakatasiloving@gmail.com
[color]
	ui = always
[push]
	default = upstream
	followTags = true
[alias]
	#d = diff --color-words --word-diff-regex='\\w+|[\\x21-\\x7F]+|[^[:space:]]' --ignore-space-change
	d = diff --color-words --word-diff-regex='\\w+|[\\x21-\\x2F\\x3A-\\x40\\x5B-\\x5E\\x60\\x7B-\\x7F]+|[^[:space:]]+' --ignore-space-change
	wdiff = diff --word-diff --ignore-space-change
	cdiff = diff --cached --ignore-space-change
	s = status --short --branch --untracked-files=all
	statu = status
	stats = status
	staut = status
	statsu = status
	stauts = status
	stastu = status
	stastus = status
	c = checkout
	chekcout = checkout
	comit = commit
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[url "git@github.com:"]
	pushInsteadOf = git://github.com/
	pushInsteadOf = https://github.com/
